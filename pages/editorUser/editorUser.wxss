/* pages/editorUser/editorUser.wxss */
.content{
   width: 100%; 
   padding-top: 100rpx;
}
.wallPaper{
  background-image: url(https://img5.imgtn.bdimg.com/it/u=67356540,1693273787&fm=27&gp=0.jpg);
  background-repeat: no-repeat;
  background-size: cover;
  position: relative;
  width: 70%; 
  height: 800rpx;
  margin: auto; 
}
.txt{
  background-color: rgba(0,0,0,0.3);
  width: 100%;
  height: 200rpx;
  position: absolute;
  top: 0;
  left: 0;
  text-align: center;
}
.name, .welcome{
  height: 100rpx;
  line-height: 100rpx;
}
.welcome{
  font-size: 32rpx;
}
.tap{
  width: 70%;
  margin: auto;
  text-align: center;
  padding-top: 50rpx;
}
.fingerPrint{
  display: flex;
  justify-content: space-between;
}
.f{
  width: 120rpx;
  height: 120rpx;
  border: 1px solid #68d3ff;
  border-radius: 50%;
  margin: 30rpx;
}
.modal{
  color: #000;
  border: 1px solid red !important;
}
/*弹出框样式 begin*/
page{
  position: relative;
}
.popUp {
  width: 500rpx;
  height: 300rpx;
  border: 1px solid red;
  border-radius: 40rpx;
  background-color: #fff;
  /* color: red; */
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
}
.radio{
  display: flex;
}
.input{
  text-align: left;
  border: 1px solid #bbb;
  margin-top: 20rpx;
}
.button{
  width:50%;
  border: 1px solid #000;
  /* text-align: center; */
  color: #68d3ff;
}
/*弹出框样式 end*/