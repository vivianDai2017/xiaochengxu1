/* pages/editorUser/editorUser.wxss */
@import "../lib/iconfont.wxss";
.content{
   width: 443rpx;
   height: 789rpx; 
   margin-top: 60rpx;
   position: relative;
}
.wallPaper{
  /* background-image: url(https://img5.imgtn.bdimg.com/it/u=67356540,1693273787&fm=27&gp=0.jpg); */
  /* background-repeat: no-repeat; */
  /* background-size: cover; */
  position: absolute;
  top: 0;
  left: 0;
  width: 443rpx; 
  height: 789rpx;
}
.txt{
  background-color: rgba(0,0,0,0.3);
  width: 100%;
  height: 180rpx;
  position: absolute;
  top: 0;
  left: 0;
  text-align: center;
}
.name{
  font-size: 40rpx;
  color:#fffefe;
  margin-top: 38rpx;
}
.welcome{
  font-size: 32rpx;
  margin-top: 25rpx;
  color: #fffefe;
}
.tap{
  font-size: 32rpx;
  color: #d8d8d8;
  margin-top: 37rpx;

}
.fingerPrint{
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin-top: 73rpx;
}
.f{
  width: 120rpx;
  height: 120rpx;
  border: 2rpx solid #d8d8d8;
  border-radius: 50%;
  text-align: center;
  line-height: 120rpx;
}
.finger{
  border-color: #03abe8;
  color: #03abe8;
}
.icon-zhiwen:before{
  font-size: 80rpx;
}
.img{
  width: 40rpx;
  height: 40rpx;
  margin: auto;
}
