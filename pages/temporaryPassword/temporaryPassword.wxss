/* pages/temporaryPassword/temporaryPassword.wxss */
/*swtich样式-start*/
/*swtich整体大小*/
.wx-switch-input{
  width:86rpx !important;
  height:50rpx !important;
}
/*白色样式（false的样式）*/
.wx-switch-input::before{
  width:86rpx !important;
  height: 50rpx !important;
   background-color: #bbb !important; 
}
/*绿色样式（true的样式）*/
.wx-switch-input::after{
  width: 40rpx !important;
  height: 40rpx !important;
  background-color: #000 !important;
  margin-top: 5rpx;
} 
/*swtich样式end*/
.inline{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.getTemPass{
  width: 100%;
  height: 500rpx;
  line-height: 500rpx;
  display: flex;
}
.choiceTime{
  display: flex;
}